{
  "name": "voidforge",
  "author": {
    "name": "GInfinity"
  },
  "version": "0.0.1",
  "description": "This is a third-party Minecraft Mod Manager.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GamingInfinite/Voidforge.git"
  },
  "main": "./src/main.js",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "electron": "^13.1.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.2",
    "bootstrap-icons": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs": "^0.0.1-security",
    "mc-curseforge-api": "^2.2.3",
    "path": "^0.12.7",
    "update-electron-app": "^2.0.1"
  },
  "scripts": {
    "transpile": "tsc .\\src\\main.ts && tsc .\\src\\eindex\\index.ts",
    "start": "yarn transpile && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": ".\\assets\\icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "voidforge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
